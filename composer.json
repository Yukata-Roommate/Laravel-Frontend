{
    "name": "yukata-roommate/laravel-frontend",
    "description": "Easy to develop frontend with Laravel",
    "version": "1.0.0",
    "keywords": [
        "yukata-roommate",
        "php",
        "laravel",
        "package",
        "frontend",
        "laravel-frontend"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yukata Roommate"
        }
    ],
    "autoload": {
        "psr-4": {
            "YukataRm\\Laravel\\Frontend\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\Frontend\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.3.0",
        "laravel/framework": "^11.0 || ^12.0",
        "yukata-roommate/laravel-view": "^1.0.0",
        "yukata-roommate/laravel-command": "^1.0.0",
        "yukata-roommate/text": "^1.0.0"
    }
}